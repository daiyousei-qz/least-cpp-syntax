{
    "name": "minimal-cpp-syntax",
    "displayName": "Minimal C++ Syntax",
    "description": "Minimal support for C++ syntax highlighting. Only to colorize tokens that the language server does not.",
    "version": "0.0.4",
    "publisher": "daiyousei-qz",
    "repository": { 
        "type": "git", 
        "url": "https://github.com/daiyousei-qz/minimal-cpp-syntax" 
    },
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "language": "cpp",
                "scopeName": "source.cpp",
                "path": "./syntaxes/cpp.tmLanguage.json"
            }
        ]
    }
}